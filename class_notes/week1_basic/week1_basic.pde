// the size fucntion creates a window 
// that is width, height pixels big
size(500,500);

//sets the size of the background
background(255,0,0);

noStroke();


// the rect funtion creates a rectangle that is drawn 
// from the upper left corner. The parameter are
// x, y, width, height
rect(10,10,30,30);

fill(255,134,156);

stroke(255,255,255);

// the thickness of a stroke/outline
strokeWeight(5);

// draw a circle ...
ellipse(250,250,100,100);

// draw the line from x1,y1,x2,y2
// width returns  the width of size
// height is 
line(0, height, width, 0);